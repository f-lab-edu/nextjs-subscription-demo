{
  "printWidth": 120,
  "endOfLine": "auto",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "plugin": ["prettier-plugin-tailwindcss"]
}
